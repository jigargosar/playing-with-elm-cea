{
  "version": 2,
  "alias": [
    "elm-done",
    "https://elm-done.functional-programmer.com"
  ],
  "builds": [
    {"src": "package.json", "use": "@now/static-build"}

  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "(.*\\.(ico|json|js))",
      "dest": "$1"
    },
    {
      "src": "/service-worker.js",
      "dest": "$1",
      "headers": {"cache-control": "s-maxage=1000"}
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
